                <View android:background="@color/black_900_7f"
                    android:id="@+id/lineLineNineOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
