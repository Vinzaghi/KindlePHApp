                    android:id="@+id/txtChangePassword"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_weight="0.94"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{adminSuccessfullyChangePasswordVM.adminSuccessfullyChangePasswordModel.txtChangePassword}"
                    style="@style/txtNunitobold20" tools:text="@string/lbl_change_password"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
