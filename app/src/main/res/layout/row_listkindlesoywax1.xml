<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listkindlesoywax1RowModel" type="com.kindleapp.app.modules.delete.data.model.Listkindlesoywax1RowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnkindlesoywax"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylecornerRadius_1">
        <TextView android:ellipsize="end"
            android:id="@+id/txtKindleSoyWax"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{listkindlesoywax1RowModel.txtKindleSoyWax}"
            style="@style/txtInterbold14" tools:text="@string/msg_kindle_soy_wax"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPrice"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{listkindlesoywax1RowModel.txtPrice}"
            style="@style/txtInterbold12" tools:text="@string/lbl_php_50"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtStock15Left"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{listkindlesoywax1RowModel.txtStock15Left}"
            style="@style/txtInterbold10" tools:text="@string/lbl_stock_15_left"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowedit"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnedit"
                android:layout_height="wrap_content"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylegreen_1">
                <TextView android:ellipsize="end"
                    android:gravity="start" android:id="@+id/txtEdit"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{listkindlesoywax1RowModel.txtEdit}"
                    style="@style/txtInterbold10_1" tools:text="@string/lbl_edit"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndelete"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylered_A700cornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtDelete"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{listkindlesoywax1RowModel.txtDelete}"
                    style="@style/txtInterbold10_1" tools:text="@string/lbl_delete"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
