<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loadingScreenOneVM" type="com.kindleapp.app.modules.loadingscreenone.data.viewmodel.LoadingScreenOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearLoadingScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_800">
        <FrameLayout android:id="@+id/frameStacklogofinalone"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_377pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_69pxh"
            android:layout_marginStart="@dimen/_69pxh"
            android:layout_marginTop="@dimen/_271pxv" android:layout_width="@dimen/_252pxh">
            <ImageView android:id="@+id/imageLogofinalOne"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_236pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_width="@dimen/_236pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_logofinal1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_logofinal1"/>
            <ImageView android:id="@+id/imageKindlewhiteOne"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_252pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_kindlewhite1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_kindlewhite1"/>
        </FrameLayout>
    </LinearLayout>
</layout>
