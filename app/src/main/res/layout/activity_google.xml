<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="googleVM" type="com.kindleapp.app.modules.google.data.viewmodel.GoogleVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearGoogle"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageClose"
            android:layout_height="@dimen/_20pxh"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="@dimen/_20pxh"
            android:scaleType="fitXY" android:src="@drawable/img_close"
            tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
        <ImageView android:id="@+id/imageGoogle"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_43pxh"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_47pxv"
            android:layout_width="@dimen/_43pxh"
            android:scaleType="fitXY" android:src="@drawable/img_google"
            tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnsignin"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignin"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_106pxh"
                android:layout_marginStart="@dimen/_106pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{googleVM.googleModel.txtSignin}"
                style="@style/txtRobotoromanregular35" tools:text="@string/lbl_sign_in"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_95pxh"
                android:layout_marginStart="@dimen/_95pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{googleVM.googleModel.txtLanguage}"
                style="@style/txtRobotoromanregular15" tools:text="@string/msg_continue_on_kin"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_300_radius_7"
                android:ellipsize="end"
                android:hint="@string/lbl_email_or_phone"
                android:id="@+id/etGroup172"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_9pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@={googleVM.googleModel.etGroup172Value}"
                android:textColorHint="@color/gray_700"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtForgotemail"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{googleVM.googleModel.txtForgotemail}"
                style="@style/txtRobotoromanregular15_2" tools:text="@string/lbl_forgot_email"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:text="@{googleVM.googleModel.txtDescription}"
                style="@style/txtRobotoromanregular13" tools:text="@string/msg_to_continue_go"/>
            <LinearLayout android:id="@+id/linearRowcreateanaccou"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCreateanaccou"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.64"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{googleVM.googleModel.txtCreateanaccou}"
                    style="@style/txtRobotoromanregular15_2" tools:text="@string/msg_create_an_accou"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_height="@dimen/_31pxv"
                    android:layout_marginStart="@dimen/_140pxh"
                    android:layout_width="@dimen/_69pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_17pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_next"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageVector"
                android:layout_gravity="start"
                android:layout_height="@dimen/_4pxh"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_315pxv"
                android:layout_width="@dimen/_5pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            <LinearLayout android:id="@+id/linearRowlanguageOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.19"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{googleVM.googleModel.txtLanguageOne}"
                    style="@style/txtRobotoromanregular11" tools:text="@string/lbl_english"/>
                <LinearLayout android:id="@+id/linearRowhelp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_140pxh"
                    android:layout_weight="0.81"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHelp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{googleVM.googleModel.txtHelp}"
                        style="@style/txtRobotoromanregular11_1" tools:text="@string/lbl_help"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrivacy"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_31pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.4"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{googleVM.googleModel.txtPrivacy}"
                        style="@style/txtRobotoromanregular11_1" tools:text="@string/lbl_privacy"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTerms"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_weight="0.35"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{googleVM.googleModel.txtTerms}"
                        style="@style/txtRobotoromanregular11_1" tools:text="@string/lbl_terms"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
