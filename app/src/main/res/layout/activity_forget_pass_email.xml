<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="forgetPassEmailVM" type="com.kindleapp.app.modules.forgetpassemail.data.viewmodel.ForgetPassEmailVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnkindlewhiteone"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_28pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_801">
        <LinearLayout android:id="@+id/linearForgetPassE"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_801">
            <ImageView android:id="@+id/imageX1"
                android:layout_gravity="start"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginEnd="@dimen/_34pxh"
                android:layout_marginStart="@dimen/_34pxh"
                android:layout_width="@dimen/_19pxh"
                android:scaleType="fitXY" android:src="@drawable/img_x1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_x1"/>
            <FrameLayout android:id="@+id/frameStackkindlewhiteone"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_218pxv"
                android:layout_marginEnd="@dimen/_34pxh"
                android:layout_marginStart="@dimen/_34pxh"
                android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_144pxh">
                <ImageView android:id="@+id/imageKindlewhiteOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_144pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_kindlewhite1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_kindlewhite1"/>
                <ImageView android:id="@+id/imageLogofinalOne"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_134pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="@dimen/_134pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logofinal1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logofinal1"/>
            </FrameLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnresetpassword"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtRESETPASSWORD"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_61pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{forgetPassEmailVM.forgetPassEmailModel.txtRESETPASSWORD}"
                    style="@style/txtPPWoodlandbold28" tools:text="@string/lbl_reset_password"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtPleaseenteryo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:text="@{forgetPassEmailVM.forgetPassEmailModel.txtPleaseenteryo}"
                    style="@style/txtInterlight11" tools:text="@string/msg_please_enter_yo"/>
                <EditText
                    android:background="@drawable/rectangle_border_black_900_radius_14"
                    android:ellipsize="end"
                    android:hint="@string/lbl_email"
                    android:id="@+id/etGroup218"
                    android:inputType="textEmailAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@={forgetPassEmailVM.forgetPassEmailModel.etGroup218Value}"
                    android:textColorHint="@color/black_900_7b"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_45pxv"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="@dimen/_240pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
