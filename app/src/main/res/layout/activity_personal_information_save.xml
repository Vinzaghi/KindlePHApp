                    android:id="@+id/txtPersonalInform"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{personalInformationSaveVM.personalInformationSaveModel.txtPersonalInform}"
                    style="@style/txtNunitobold20" tools:text="@string/msg_personal_inform"/>
