<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="personalInformationVM" type="com.kindleapp.app.modules.personalinformation.data.viewmodel.PersonalInformationVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_99pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylelime_100">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylegreen_801cornerRadius">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_139pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPersonalInform"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtPersonalInform}"
                    style="@style/txtNunitobold20" tools:text="@string/msg_personal_inform"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearPersonalInform"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_38pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylelime_100">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearFirstName"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFirstName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtFirstName}"
                    style="@style/txtInterbold16_6" tools:text="@string/lbl_first_name"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHer"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtSampleNameHer}"
                    style="@style/txtInterlight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearLastName"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLastName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtLastName}"
                    style="@style/txtInterbold16_6" tools:text="@string/lbl_last_name"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHerOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtSampleNameHerOne}"
                    style="@style/txtInterlight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmailAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtEmailAddress}"
                    style="@style/txtInterbold16_6" tools:text="@string/lbl_email_address"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHerTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtSampleNameHerTwo}"
                    style="@style/txtInterlight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearContactNumber"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContactNumber"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtContactNumber}"
                    style="@style/txtInterbold16_6" tools:text="@string/lbl_contact_number"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHerThree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtSampleNameHerThree}"
                    style="@style/txtInterlight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSex"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSex"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtSex}"
                    style="@style/txtInterbold16_6" tools:text="@string/lbl_sex"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHerFour"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtSampleNameHerFour}"
                    style="@style/txtInterlight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearEmergencyConta"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmergencyConta"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtEmergencyConta}"
                    style="@style/txtInterbold16_6" tools:text="@string/msg_emergency_conta"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHerFive"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{personalInformationVM.personalInformationModel.txtSampleNameHerFive}"
                    style="@style/txtInterlight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackemergencycontaOne"
                android:layout_height="@dimen/_139pxv"
                android:layout_marginTop="@dimen/_20pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearEmergencyConta1"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_14">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmergencyContaOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{personalInformationVM.personalInformationModel.txtEmergencyContaOne}"
                        style="@style/txtInterbold16_6" tools:text="@string/msg_emergency_conta2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSampleNameHerSix"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{personalInformationVM.personalInformationModel.txtSampleNameHerSix}"
                        style="@style/txtInterlight18" tools:text="@string/msg_sample_name_her"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSave"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_marginEnd="@dimen/_115pxh"
                    android:layout_marginStart="@dimen/_115pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_70pxh"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_14pxh"
                    android:paddingStart="@dimen/_17pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_save"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_15"
                    textUnderline="true" tools:text="@string/lbl_save"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnClear"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_marginEnd="@dimen/_120pxh"
                    android:layout_marginStart="@dimen/_120pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_70pxh"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_13pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_clear"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_16"
                    textUnderline="true" tools:text="@string/lbl_clear"/>
                <LinearLayout android:id="@+id/linearRowhomepage"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylegreen_801cornerRadius">
                    <ImageView android:id="@+id/imageHomePage"
                        android:layout_height="@dimen/_37pxh"
                        android:layout_marginBottom="@dimen/_37pxv"
                        android:layout_marginStart="@dimen/_49pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_homepage"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_homepage"/>
                    <ImageView android:id="@+id/imageOnlineStore"
                        android:layout_height="@dimen/_37pxh"
                        android:layout_marginBottom="@dimen/_37pxv"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_onlinestore"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_onlinestore"/>
                    <ImageView android:id="@+id/imageShoppingCart"
                        android:layout_height="@dimen/_37pxh"
                        android:layout_marginBottom="@dimen/_37pxv"
                        android:layout_marginStart="@dimen/_49pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shoppingcart"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shoppingcart"/>
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_37pxh"
                        android:layout_marginBottom="@dimen/_37pxv"
                        android:layout_marginEnd="@dimen/_44pxh"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_37x37"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_37x37"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
