<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="paymentGcashVM" type="com.kindleapp.app.modules.paymentgcash.data.viewmodel.PaymentGcashVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearPaymentGcash"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylelime_100">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStylegreen_801cornerRadius">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_103pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAddingPayment"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_weight="0.96"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{paymentGcashVM.paymentGcashModel.txtAddingPayment}"
                        style="@style/txtNunitobold20" tools:text="@string/msg_adding_payment"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnaccountname"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAccountName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtAccountName}"
                    style="@style/txtNunitobold16" tools:text="@string/lbl_account_name"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHer"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtSampleNameHer}"
                    style="@style/txtNunitolight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <View android:background="@color/green_801"
                android:id="@+id/lineLineTwelve"
                android:layout_gravity="start"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_320pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnmobilenumber"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMobileNumber"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtMobileNumber}"
                    style="@style/txtNunitobold16" tools:text="@string/lbl_mobile_number"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEnteryour11d"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtEnteryour11d}"
                    style="@style/txtNunitolight18" tools:text="@string/msg_enter_your_11_d"/>
            </LinearLayout>
            <View android:background="@color/green_801"
                android:id="@+id/lineLineEleven"
                android:layout_gravity="start"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_320pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnpaymentmethod"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPaymentMethod"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtPaymentMethod}"
                    style="@style/txtNunitobold16" tools:text="@string/lbl_payment_method"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGCash"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtGCash}"
                    style="@style/txtNunitolight18" tools:text="@string/lbl_gcash"/>
            </LinearLayout>
            <View android:background="@color/green_801"
                android:id="@+id/lineLineTen"
                android:layout_gravity="start"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_320pxh"/>
            <LinearLayout android:id="@+id/linearColumnclear"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowclear"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnClear"
                        android:layout_height="@dimen/_34pxv"
                        android:layout_width="@dimen/_70pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_15pxh"
                        android:paddingStart="@dimen/_15pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_clear"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_18"
                        textUnderline="true" tools:text="@string/lbl_clear"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSave"
                        android:layout_height="@dimen/_34pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_70pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_17pxh"
                        android:paddingStart="@dimen/_17pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_save"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_18"
                        textUnderline="true" tools:text="@string/lbl_save"/>
                </LinearLayout>
                <TextView android:gravity="start"
                    android:id="@+id/txtPreferredPayme"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_35pxv"
                    android:layout_width="match_parent"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtPreferredPayme}"
                    style="@style/txtNunitobold20_2" tools:text="@string/msg_preferred_payme"/>
                <TextView android:id="@+id/txtNoneedtofret"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:text="@{paymentGcashVM.paymentGcashModel.txtNoneedtofret}"
                    style="@style/txtNunitobold12" tools:text="@string/msg_no_need_to_fret"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegreen_801cornerRadius">
            <LinearLayout android:id="@+id/linearRowhomepage"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_37pxv"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHomePage"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_homepage"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_homepage"/>
                <ImageView android:id="@+id/imageOnlineStore"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_50pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_onlinestore"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_onlinestore"/>
                <ImageView android:id="@+id/imageShoppingCart"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_shoppingcart"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_shoppingcart"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginEnd="@dimen/_44pxh"
                    android:layout_marginStart="@dimen/_50pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_37x37"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_37x37"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
