<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="adminChangePasswordVM" type="com.kindleapp.app.modules.adminchangepassword.data.viewmodel.AdminChangePasswordVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearAdminChange"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylelime_100">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnarrowleft"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStylegreen_801cornerRadius">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_167pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChangePassword"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_weight="0.94"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{adminChangePasswordVM.adminChangePasswordModel.txtChangePassword}"
                        style="@style/txtNunitobold20" tools:text="@string/lbl_change_password"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOldPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_51pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{adminChangePasswordVM.adminChangePasswordModel.txtOldPassword}"
                style="@style/txtNunitobold20_2" tools:text="@string/lbl_old_password"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_11"
                android:ellipsize="end"
                android:hint="@string/lbl_qwerty_12345"
                android:id="@+id/etGroup206" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_3pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_9pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@={adminChangePasswordVM.adminChangePasswordModel.etGroup206Value}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline_2" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{adminChangePasswordVM.adminChangePasswordModel.txtNewPassword}"
                style="@style/txtNunitobold20_2" tools:text="@string/lbl_new_password"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_11"
                android:ellipsize="end"
                android:hint="@string/lbl_12345_qwerty"
                android:id="@+id/etGroup205" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_3pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_9pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@={adminChangePasswordVM.adminChangePasswordModel.etGroup205Value}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline_2" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtConfirmPasswor"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{adminChangePasswordVM.adminChangePasswordModel.txtConfirmPasswor}"
                style="@style/txtNunitobold20_2" tools:text="@string/msg_confirm_passwor"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_11"
                android:ellipsize="end"
                android:hint="@string/lbl_12345_qwerty"
                android:id="@+id/etGroup204" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_3pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_9pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@={adminChangePasswordVM.adminChangePasswordModel.etGroup204Value}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline_2" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSave"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_24pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_78pxh"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_28pxh"
                android:paddingStart="@dimen/_25pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true"
                android:text="@string/lbl_save"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_5" tools:text="@string/lbl_save"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowlineeight"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View android:background="@color/green_801"
                    android:id="@+id/lineLineEight"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.48" android:layout_width="0dp"/>
                <ImageView android:id="@+id/imageCall"
                    android:layout_height="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_call"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                <View android:background="@color/green_801"
                    android:id="@+id/lineLineNine"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.48" android:layout_width="0dp"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnDeleteAccount"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_24pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="@dimen/_93pxh"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_8pxh"
                android:paddingStart="@dimen/_9pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true"
                android:text="@string/lbl_delete_account"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_7" tools:text="@string/lbl_delete_account"/>
            <View android:background="@color/black_900_7f"
                android:id="@+id/lineLineNineOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_213pxv" android:layout_width="@dimen/_325pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{adminChangePasswordVM.adminChangePasswordModel.txtLanguage}"
                style="@style/txtInterregular8" tools:text="@string/msg_kindle_ph_e_com"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegreen_801cornerRadius">
            <LinearLayout android:id="@+id/linearRowhomepage"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_37pxv"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHomePage"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_homepage"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_homepage"/>
                <ImageView android:id="@+id/imageProduct"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_product"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_product"/>
                <ImageView android:id="@+id/imageTotalSales"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_totalsales"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_totalsales"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginEnd="@dimen/_44pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_37x37"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_37x37"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
