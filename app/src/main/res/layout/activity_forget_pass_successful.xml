<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="forgetPassSuccessfulVM" type="com.kindleapp.app.modules.forgetpasssuccessful.data.viewmodel.ForgetPassSuccessfulVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnkindlewhiteone"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_87pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_801">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearForgetPassS"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_801">
            <FrameLayout android:id="@+id/frameStackkindlewhiteone"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_218pxv"
                android:layout_marginEnd="@dimen/_123pxh"
                android:layout_marginStart="@dimen/_123pxh" android:layout_width="@dimen/_144pxh">
                <ImageView android:id="@+id/imageKindlewhiteOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_144pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_kindlewhite1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_kindlewhite1"/>
                <ImageView android:id="@+id/imageLogofinalOne"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_134pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="@dimen/_134pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logofinal1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logofinal1"/>
            </FrameLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsuccessful"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSUCCESSFUL"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_61pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{forgetPassSuccessfulVM.forgetPassSuccessfulModel.txtSUCCESSFUL}"
                    style="@style/txtPPWoodlandbold28" tools:text="@string/lbl_successful"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:text="@{forgetPassSuccessfulVM.forgetPassSuccessfulModel.txtDescription}"
                    style="@style/txtInterlight11" tools:text="@string/msg_your_password_h"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnGoBack"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_45pxv"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_38pxv"
                    android:layout_width="@dimen/_240pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_go_back"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_go_back"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
