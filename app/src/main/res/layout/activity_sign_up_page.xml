<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpPageVM" type="com.kindleapp.app.modules.signuppage.data.viewmodel.SignUpPageVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnkindlewhiteone"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_28pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_801">
        <LinearLayout android:id="@+id/linearSignUpPage"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_801">
            <ImageView android:id="@+id/imageX1"
                android:layout_gravity="start"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginEnd="@dimen/_34pxh"
                android:layout_marginStart="@dimen/_34pxh"
                android:layout_width="@dimen/_19pxh"
                android:scaleType="fitXY" android:src="@drawable/img_x1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_x1"/>
            <FrameLayout android:id="@+id/frameStackkindlewhiteone"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_218pxv"
                android:layout_marginEnd="@dimen/_34pxh"
                android:layout_marginStart="@dimen/_34pxh"
                android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_144pxh">
                <ImageView android:id="@+id/imageKindlewhiteOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_144pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_kindlewhite1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_kindlewhite1"/>
                <ImageView android:id="@+id/imageLogofinalOne"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_134pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="@dimen/_134pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logofinal1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logofinal1"/>
            </FrameLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsignup"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSIGNUP"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_48pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signUpPageVM.signUpPageModel.txtSIGNUP}"
                    style="@style/txtPPWoodlandbold29" tools:text="@string/lbl_sign_up2"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumngroup283"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_38pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowgroup283"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <EditText
                            android:background="@drawable/rectangle_border_black_900_radius_14"
                            android:ellipsize="end"
                            android:hint="@string/lbl_first_name"
                            android:id="@+id/etGroup283"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_11pxh"
                            android:paddingTop="@dimen/_9pxv"
                            android:singleLine="true"
                            android:text="@={signUpPageVM.signUpPageModel.etGroup283Value}"
                            android:textColorHint="@color/black_900_7b"
                            style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                        <EditText
                            android:background="@drawable/rectangle_border_black_900_radius_14"
                            android:ellipsize="end"
                            android:hint="@string/lbl_last_name"
                            android:id="@+id/etGroup282"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_9pxh"
                            android:paddingTop="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@={signUpPageVM.signUpPageModel.etGroup282Value}"
                            android:textColorHint="@color/black_900_7b"
                            style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <EditText
                        android:background="@drawable/rectangle_border_black_900_radius_14"
                        android:ellipsize="end"
                        android:hint="@string/lbl_email"
                        android:id="@+id/etGroup281"
                        android:inputType="textEmailAddress"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_7pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_10pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@={signUpPageVM.signUpPageModel.etGroup281Value}"
                        android:textColorHint="@color/black_900_7b"
                        style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                    <EditText
                        android:background="@drawable/rectangle_border_black_900_radius_14"
                        android:ellipsize="end"
                        android:hint="@string/lbl_password"
                        android:id="@+id/etGroup280"
                        android:inputType="textPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_10pxh"
                        android:paddingTop="@dimen/_9pxv"
                        android:singleLine="true"
                        android:text="@={signUpPageVM.signUpPageModel.etGroup280Value}"
                        android:textColorHint="@color/black_900_7b"
                        style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                    <EditText
                        android:background="@drawable/rectangle_border_black_900_radius_14"
                        android:ellipsize="end"
                        android:hint="@string/msg_confirm_passwor"
                        android:id="@+id/etGroup279"
                        android:inputType="textPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_9pxh"
                        android:paddingTop="@dimen/_9pxv"
                        android:singleLine="true"
                        android:text="@={signUpPageVM.signUpPageModel.etGroup279Value}"
                        android:textColorHint="@color/black_900_7b"
                        style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowrectangle4278"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <View
                            android:background="@drawable/rectangle_border_black_900_radius_6"
                            android:id="@+id/viewRectangle4278"
                            android:layout_height="@dimen/_18pxv"
                            android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_23pxh"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtIAgreewithth"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.9"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{signUpPageVM.signUpPageModel.txtIAgreewithth}"
                            style="@style/txtInterregular13" tools:text="@string/msg_i_agree_with_th"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSignUpOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_sign_up3"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_2" tools:text="@string/lbl_sign_up3"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtConfirmation"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_78pxh"
                    android:layout_marginStart="@dimen/_78pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{signUpPageVM.signUpPageModel.txtConfirmation}"
                    style="@style/txtNunitobold13_1" tools:text="@string/msg_already_have_an"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
