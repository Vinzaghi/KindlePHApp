<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="logInAdminVM" type="com.kindleapp.app.modules.loginadmin.data.viewmodel.LogInAdminVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnkindlewhiteone"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_87pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_801">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearLogInAdmin"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_801">
            <FrameLayout android:id="@+id/frameStackkindlewhiteone"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_218pxv"
                android:layout_marginEnd="@dimen/_123pxh"
                android:layout_marginStart="@dimen/_123pxh" android:layout_width="@dimen/_144pxh">
                <ImageView android:id="@+id/imageKindlewhiteOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_144pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_kindlewhite1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_kindlewhite1"/>
                <ImageView android:id="@+id/imageLogofinalOne"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_134pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="@dimen/_134pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logofinal1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logofinal1"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearColumnadminlogin"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtADMINLOGIN"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_61pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{logInAdminVM.logInAdminModel.txtADMINLOGIN}"
                    style="@style/txtPPWoodlandbold28" tools:text="@string/lbl_admin_log_in"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtWelcomeAdminis"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent"
                    android:text="@{logInAdminVM.logInAdminModel.txtWelcomeAdminis}"
                    style="@style/txtInterlight11" tools:text="@string/msg_welcome_adminis"/>
                <EditText
                    android:background="@drawable/rectangle_border_black_900_radius_14"
                    android:drawablePadding="@dimen/_12pxh"
                    android:drawableStart="@drawable/img_user"
                    android:ellipsize="end"
                    android:hint="@string/lbl_username"
                    android:id="@+id/etGroup263"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_14pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@={logInAdminVM.logInAdminModel.etGroup263Value}"
                    android:textColorHint="@color/black_900_7b"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_border_black_900_radius_14"
                    android:drawablePadding="@dimen/_11pxh"
                    android:drawableStart="@drawable/img_lock"
                    android:ellipsize="end"
                    android:hint="@string/lbl_password"
                    android:id="@+id/etGroup262"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@={logInAdminVM.logInAdminModel.etGroup262Value}"
                    android:textColorHint="@color/black_900_7b"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtForgotPassword"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_64pxh"
                    android:layout_marginStart="@dimen/_64pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{logInAdminVM.logInAdminModel.txtForgotPassword}"
                    style="@style/txtNunitobold11" tools:text="@string/msg_forgot_password"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnLogIn"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_45pxv"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_240pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_log_in3"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_log_in3"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoginasUser"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{logInAdminVM.logInAdminModel.txtLoginasUser}"
                    style="@style/txtNunitobold13_1" tools:text="@string/lbl_log_in_as_user"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
