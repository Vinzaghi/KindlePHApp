<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="paymentDebitCreditVM" type="com.kindleapp.app.modules.paymentdebitcredit.data.viewmodel.PaymentDebitCreditVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_166pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylelime_100">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylegreen_801cornerRadius">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_103pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddingPayment"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_weight="0.96"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtAddingPayment}"
                    style="@style/txtNunitobold20" tools:text="@string/msg_adding_payment"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearPaymentDebit"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylelime_100">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnaccountname"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAccountName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtAccountName}"
                    style="@style/txtNunitobold16" tools:text="@string/lbl_account_name"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSampleNameHer"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtSampleNameHer}"
                    style="@style/txtNunitolight18" tools:text="@string/msg_sample_name_her"/>
            </LinearLayout>
            <View android:background="@color/green_801"
                android:id="@+id/lineLineTwelve"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_320pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumncardnumber"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCardNumber"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtCardNumber}"
                    style="@style/txtNunitobold16" tools:text="@string/lbl_card_number"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt000000000000"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txt000000000000}"
                    style="@style/txtNunitolight18" tools:text="@string/msg_0000_0000_0000"/>
            </LinearLayout>
            <View android:background="@color/green_801"
                android:id="@+id/lineLineEleven"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_320pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnpaymentmethod"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_14">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPaymentMethod"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtPaymentMethod}"
                    style="@style/txtNunitobold16" tools:text="@string/lbl_payment_method"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDebitCredit"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtDebitCredit}"
                    style="@style/txtNunitolight18" tools:text="@string/msg_debit_credit"/>
            </LinearLayout>
            <View android:background="@color/green_801"
                android:id="@+id/lineLineTen"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_320pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowexpiration"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnexpiration"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylewhite_14">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtExpiration"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtExpiration}"
                        style="@style/txtNunitobold16" tools:text="@string/lbl_expiration"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMMYY"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtMMYY}"
                        style="@style/txtNunitolight18" tools:text="@string/lbl_mm_yy"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumncvv"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylewhite_14">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCVV"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtCVV}"
                        style="@style/txtNunitobold16" tools:text="@string/lbl_cvv"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOneHundredTwentyThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtOneHundredTwentyThree}"
                        style="@style/txtNunitolight18" tools:text="@string/lbl_123"/>
                </LinearLayout>
            </LinearLayout>
            <View android:background="@color/green_801"
                android:id="@+id/lineLineThirteen"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_320pxh"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnclear"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowclear"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_110pxh"
                    android:layout_marginStart="@dimen/_110pxh"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnClear"
                        android:layout_height="@dimen/_34pxv"
                        android:layout_width="@dimen/_70pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_15pxh"
                        android:paddingStart="@dimen/_15pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_clear"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_18"
                        textUnderline="true" tools:text="@string/lbl_clear"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSave"
                        android:layout_height="@dimen/_34pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_70pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_17pxh"
                        android:paddingStart="@dimen/_17pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_save"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_18"
                        textUnderline="true" tools:text="@string/lbl_save"/>
                </LinearLayout>
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackpreferredpayme"
                    android:layout_height="@dimen/_126pxv"
                    android:layout_marginTop="@dimen/_41pxv" android:layout_width="match_parent">
                    <TextView android:id="@+id/txtPreferredPayme"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_width="wrap_content"
                        android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtPreferredPayme}"
                        style="@style/txtNunitobold20_2" tools:text="@string/msg_preferred_payme"/>
                    <TextView android:id="@+id/txtNoneedtofret"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="wrap_content"
                        android:text="@{paymentDebitCreditVM.paymentDebitCreditModel.txtNoneedtofret}"
                        style="@style/txtNunitobold12" tools:text="@string/msg_no_need_to_fret"/>
                    <LinearLayout android:id="@+id/linearRowhomepage"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylegreen_801cornerRadius">
                        <ImageView android:id="@+id/imageHomePage"
                            android:layout_height="@dimen/_37pxh"
                            android:layout_marginBottom="@dimen/_37pxv"
                            android:layout_marginStart="@dimen/_49pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="@dimen/_37pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_homepage"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_homepage"/>
                        <ImageView android:id="@+id/imageOnlineStore"
                            android:layout_height="@dimen/_37pxh"
                            android:layout_marginBottom="@dimen/_37pxv"
                            android:layout_marginStart="@dimen/_50pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="@dimen/_37pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_onlinestore"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_onlinestore"/>
                        <ImageView android:id="@+id/imageShoppingCart"
                            android:layout_height="@dimen/_37pxh"
                            android:layout_marginBottom="@dimen/_37pxv"
                            android:layout_marginStart="@dimen/_49pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="@dimen/_37pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_shoppingcart"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_shoppingcart"/>
                        <ImageView android:id="@+id/imageUser"
                            android:layout_height="@dimen/_37pxh"
                            android:layout_marginBottom="@dimen/_37pxv"
                            android:layout_marginEnd="@dimen/_44pxh"
                            android:layout_marginStart="@dimen/_50pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="@dimen/_37pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user_37x37"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user_37x37"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
